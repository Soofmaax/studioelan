{
  "name": "studio-elan",
  "version": "1.0.0",
  "description": "Application web fullstack moderne pour un studio de yoga premium - Next.js + NestJS",
  "private": true,
  "author": {
    "name": "Studio Ã‰lan",
    "email": "contact@studio-elan.fr"
  },
  "keywords": [
    "yoga",
    "wellness",
    "nextjs",
    "nestjs",
    "typescript",
    "fullstack",
    "premium"
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "cd src && nest start --watch",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "next build",
    "build:backend": "cd src && nest build",
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "next start",
    "start:backend": "cd src && node dist/main",
    "lint": "next lint && cd src && eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "npm run lint -- --fix",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "prepare": "husky install",
    "validate": "npm run type-check && npm run lint && npm run test && npm run test:e2e",
    "clean": "rimraf .next dist coverage test-results",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml}\""
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/terminus": "^10.2.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.12.0",
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-slot": "^1.2.2",
    "@sentry/nextjs": "^7.108.0",
    "@stripe/stripe-js": "^3.0.10",
    "@tanstack/react-query": "^5.28.4",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "clsx": "^2.1.0",
    "concurrently": "^8.2.2",
    "date-fns": "^3.6.0",
    "framer-motion": "^11.0.14",
    "lucide-react": "^0.358.0",
    "next": "^14.1.3",
    "next-auth": "^4.24.7",
    "next-themes": "^0.3.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "react": "^18.2.0",
    "react-big-calendar": "^1.10.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.1",
    "react-hot-toast": "^2.4.1",
    "reflect-metadata": "^0.2.1",
    "stripe": "^14.20.0",
    "swagger-ui-express": "^5.0.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@nestjs/cli": "^10.3.0",
    "@nestjs/schematics": "^10.1.0",
    "@nestjs/testing": "^10.3.0",
    "@playwright/test": "^1.42.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.28",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prisma": "^5.12.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "jest": {
    "projects": [
      {
        "displayName": "frontend",
        "testEnvironment": "jsdom",
        "setupFilesAfterEnv": [
          "<rootDir>/jest.setup.js"
        ],
        "testMatch": [
          "<rootDir>/components/**/*.test.{js,ts,tsx}",
          "<rootDir>/app/**/*.test.{js,ts,tsx}",
          "<rootDir>/__tests__/**/*.test.{js,ts,tsx}",
          "<rootDir>/hooks/**/*.test.{js,ts,tsx}",
          "<rootDir>/lib/**/*.test.{js,ts,tsx}"
        ],
        "moduleNameMapper": {
          "^@/(.*)$": "<rootDir>/$1"
        },
        "transform": {
          "^.+\\.(js|jsx|ts|tsx)$": [
            "babel-jest",
            {
              "presets": [
                "next/babel"
              ]
            }
          ]
        }
      },
      {
        "displayName": "backend",
        "testEnvironment": "node",
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
          "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
          "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "moduleNameMapper": {
          "^@/(.*)$": "<rootDir>/$1"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}

